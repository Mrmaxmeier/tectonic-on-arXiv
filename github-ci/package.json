{
  "name": "tectonic-ci",
  "version": "1.0.0",
  "description": "A Probot app",
  "author": "Mrmaxmeier <Mrmaxmeier@gmail.com>",
  "license": "ISC",
  "repository": "https://github.com//tectonic-on-arXiv.git",
  "homepage": "https://github.com//tectonic-on-arXiv",
  "bugs": "https://github.com//tectonic-on-arXiv/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "nodemon --exec \"npm start\"",
    "start": "probot run ./lib/index.js",
    "lint": "standard **/*.ts --fix",
    "test": "jest && standard **/*.ts",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "@types/nock": "^11.1.0",
    "@types/nodegit": "^0.24.12",
    "nock": "^11.6.0",
    "nodegit": "^0.26.2",
    "probot": "^9.6.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "@types/node": "^12.11.7",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^24.0.0",
    "nodemon": "^1.17.2",
    "smee-client": "^1.0.2",
    "standard": "^14.3.1",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.1",
    "typescript-eslint-parser": "^22.0.0"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "parser": "typescript-eslint-parser",
    "env": [
      "jest"
    ],
    "plugins": [
      "typescript"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
